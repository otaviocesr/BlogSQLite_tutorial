<table>
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nome</th>
      <th scope="col">Email</th>
      <th scope="col">Celular</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 1; i < 11; i++) { %> <% if(i % 3 == 0) { %>
    <tr>
      <td><%= i %></td>
      <td>Nome</td>
      <td>Email</td>
      <td>Celular</td>
    </tr>
    <% } %> <% } %>
  </tbody>
</table>
