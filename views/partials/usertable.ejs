<% console.log(`usertable.ejs -> DADOS: ${JSON.stringify(dados)}`) %>
<table>
  <thead class="usertable">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nome</th>
      <th scope="col">Email</th>
      <th scope="col">Celular</th>
      <th scope="col">Senha</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < dados.length; i++) { %>

      <tr <%- req.session.username===dados[i].username ? `style="color: white; font-style: italic;" ` : `` %>>

        <td><%= dados[i].id %></td>
        <td><%= dados[i].username %></td>
        <td><%= dados[i].email %></td>
        <td><%= dados[i].tel %></td>
        <td><%= dados[i].password %></td>
      </tr>
    <% } %>

  </tbody>
  <tfoot>
    <tr>

      <th colspan="5" style="font-size: 16px;"><%=  `UsuÃ¡rios registrados: ${dados.length}` %></th>
    </tr>
  </tfoot>
</table>

